              }
            ]
          each desc in descs
            li.desc__item
              positions
                ul.positions__list
                  li(
                    class=`desc__item-${desc.name}`
                    ).desc__item #{desc.content}                    
                  li(
                    class=`desc__item-${desc.name}`
                  ).desc__item #{desc.content1}
                  li(
                    class=`desc__item-${desc.name}`
                  ).desc__item #{desc.content2}
                  li(
                    class=`desc__item-${desc.name}`
                  ).desc__item #{desc.content3}
                  li(
                    class=`desc__item-${desc.name}`
                  ).desc__item #{desc.content4}
